{
  "name": "sass-project",
  "nativescript": {
    "id": "org.nativescript.sassproject",
    "tns-android": {
      "version": "6.5.1"
    }
  },
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall:web && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --dev-server-target=",
    "android": "tns run android",
    "android:debug": "tns debug android",
    "ios": "tns run ios",
    "mobile": "tns run",
    "preview": "tns preview",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node scripts/electron-build/postinstall-web",
    "postinstall:electron": "node scripts/electron-build/postinstall",
    "electron:start": "npm-run-all -p electron:serve electron:start",
    "electron:serve-tsc": "tsc -p tsconfig.electron.json",
    "electron:serve": "npm run postinstall:electron && ng serve -c electronDev",
    "electron:open": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && npm run electron:windows:onlyElectronBuild",
    "electron:windows:onlyElectronBuild": "npm run electron:serve-tsc && electron-builder build --windows --publish always",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "electron:build": "npm run postinstall:electron && npm run electron:windows"
  },
  "private": true,
  "main": "scripts/electron-run/main.js",
  "dependencies": {
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^4.3.1",
    "nativescript-ui-sidedrawer": "^8.0.1",
    "stream-to-blob": "^2.0.1",
    "tns-core-modules": "~6.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.0",
    "@angular/animations": "~8.2.0",
    "@angular/cli": "~8.3.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/http": "~8.0.0-beta.10",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/pwa": "^0.901.7",
    "@angular/router": "~8.2.0",
    "@angular/service-worker": "~8.2.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nativescript/schematics": "~1.0.0",
    "@nativescript/theme": "~2.2.1",
    "@nativescript/tslint-rules": "~0.0.5",
    "@ngx-translate/core": "^12.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.5.0",
    "codelyzer": "^5.0.0",
    "core-js": "^2.6.9",
    "electron": "^9.0.1",
    "electron-builder": "^22.7.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.26.0",
    "nativescript-angular": "~8.21.0",
    "nativescript-dev-webpack": "~1.4.0",
    "ngx-cookie-service": "^3.0.4",
    "node-sass": "^4.7.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~6.4.0",
    "ts-node": "~7.0.0",
    "tslib": "1.10.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "wait-on": "^5.0.0",
    "zone.js": "~0.9.1"
  }
}
