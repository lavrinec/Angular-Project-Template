{
  "name": "project-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall:web && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --dev-server-target=",
    "android": "tns run android",
    "android:debug": "tns debug android",
    "ios": "tns run ios",
    "mobile": "tns run",
    "preview": "tns preview",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall:tns": "npm run ngcc",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node scripts/electron-build/postinstall-web",
    "postinstall:electron": "node scripts/electron-build/postinstall",
    "electron:start": "npm-run-all -p electron:serve electron:start",
    "electron:serve-tsc": "tsc -p tsconfig.electron.json",
    "electron:serve": "npm run postinstall:electron && ng serve -c electronDev",
    "electron:open": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && npm run electron:windows:onlyElectronBuild",
    "electron:windows:onlyElectronBuild": "npm run electron:serve-tsc && electron-builder build --windows --publish always",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "electron:build": "npm run postinstall:electron && npm run electron:windows"
  },
  "private": true,
  "main": "scripts/electron-run/main.js",
  "dependencies": {
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^4.3.1",
    "nativescript-ui-sidedrawer": "^9.0.0-rc.0",
    "stream-to-blob": "^2.0.1",
    "@nativescript/core": "^7.0.0-rc.20"
  },
  "devDependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "core-js": "^2.6.9",
    "@nativescript/angular": "10.0.0-rc.3",
    "@nativescript/theme": "^2.3.3",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~6.5.5",
    "tslib": "1.10.0",
    "zone.js": "~0.10.3",
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@nativescript/tslint-rules": "~0.0.5",
    "@nativescript/webpack": "^2.0.0-rc.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "node-sass": "^4.7.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "nativescript": {
    "id": "org.nativescript.sassprojectangular10",
    "tns-android": {
      "version": "6.5.3"
    }
  }
}
