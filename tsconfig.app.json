{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "module": "es2015",
    "types": [],
    "paths": {
      "@src/*": [
        "src/*.web",
        "src/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ]
}
